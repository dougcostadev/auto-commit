{
  "version": "1.0.0",
  "batchSizes": {
    "binary": 500,
    "media": 300,
    "assets": 1000,
    "archives": 200,
    "source": 1500,
    "web": 1200,
    "mobile": 1000,
    "database": 500,
    "config": 800,
    "docs": 1000,
    "data": 500,
    "system": 2000,
    "misc": 1000
  },
  "excludePatterns": [
    "node_modules/**",
    ".git/**",
    "dist/**",
    "build/**",
    ".cache/**",
    ".tmp/**",
    "*.log"
  ],
  "commitMessage": {
    "template": "feat: add {count} {type} files - {description}",
    "useConventional": true
  },
  "processing": {
    "parallel": true,
    "maxConcurrency": 3,
    "retryFailedPushes": true,
    "skipLargeFiles": true,
    "maxFileSize": 52428800
  },
  "maxPushSize": 1073741824,
  "fileTypes": {
    "binary": {
      "name": "Binary Files",
      "description": "Executable files, compiled binaries, and machine code",
      "extensions": [
        ".exe",
        ".dll",
        ".so",
        ".dylib",
        ".bin",
        ".app"
      ],
      "patterns": [
        "*.exe",
        "*.dll",
        "*.so",
        "*.dylib",
        "*.bin",
        "*.app"
      ],
      "batchSize": 500,
      "icon": "‚öôÔ∏è"
    },
    "media": {
      "name": "Media Files",
